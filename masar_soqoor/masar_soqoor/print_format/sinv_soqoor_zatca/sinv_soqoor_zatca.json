 "html": "{%- set items_per_page = 10 -%}\r\n{%- set total_pages = ((doc.items|length / items_per_page)|round(0, 'ceil')|int) -%}\r\n{%- set access_log_doc = frappe.get_all('Access Log', filters={'export_from': 'Sales Invoice',\r\n                                                               'reference_document': doc.name,\r\n                                                               'method': 'Print'}) -%}\r\n{%- set access_log_count = access_log_doc|length -%}\r\n{%- for page in range(total_pages)-%}\r\n<br>\r\n<br><br>\r\n<div class=\"row\">\r\n    <div class=\"col-xs-3 text-left\" id=\"page-number\" style=\"text-align: right; text-left: 0px;font-size: 50\">\r\n        <b>Page {{ page + 1 }} of {{ total_pages }}</b>\r\n    </div>\r\n</div>\r\n{%- if loop.first -%}\r\n    {%- if access_log_count == 1 -%}\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-6 text-right\" id=\"type\">\r\n            <b>Original</b>\r\n        </div>\r\n    </div>\r\n    {%- elif access_log_count > 1 -%}\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-6 text-right\" id=\"type\">\r\n            <b>Copy {{ access_log_count - 1 }}</b>\r\n        </div>\r\n    </div>\r\n    {%- endif -%}\r\n{%- endif -%}\r\n<br><br>\r\n\r\n<div class=\"row\">\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{ '' }}</b></div>\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: left; text-left: 20px; font-size: 50\"><b>{{doc.custom_serial_no or ''}}</b></div>\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{ doc.payment_type }}</b></div>\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{doc.posting_date or ''}}</b></div>\r\n        </div>\r\n        <br>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 45\"><b>{{doc.po_no or ''}}</b></div>\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{doc.contact_mobile or ''}}</b></div>\r\n            <div class=\"col-xs-5 text-left\" style=\"text-align: right; text-left: 0px;font-size: 50\"><b>{{doc.customer_name or '' }}</b></div>\r\n        </div>\r\n        <br>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;font-size: 100\"><b>{{doc.tax_id or ''}}</b></div>\r\n        </div>\r\n<br><br>\r\n\r\n<table class=\"table table-bordered border-style: none;\">\r\n\t<tbody>\r\n\t\t<!--<tr>-->\r\n\t\t<!--\t<th>{{ _(\"Sr\") }}</th>-->\r\n\t\t<!--\t<th>{{ _(\"Description\") }}</th>-->\r\n\t\t<!--\t<th class=\"text-center\">{{ _(\"Item Code\") }}</th>-->\r\n\t\t<!--\t<th class=\"text-center\">{{ _(\"Quantity\") }}</th>-->\r\n\t\t<!--\t<th class=\"text-center\">{{ _(\"Unit Price\") }}</th>-->\r\n\t\t<!--\t<th class=\"text-center\">{{ _(\"Net Total Befor Dis\") }}</th>-->\r\n\t\t<!--\t<th class=\"text-center\">{{ _(\"Total After Discount\") }}</th>-->\r\n\t\t<!--\t<th class=\"text-center\">{{ _(\"Quantity Processed\") }}</th>-->\r\n\t\t<!--</tr>-->\r\n\t\t{%- for row in doc.items[page * items_per_page:(page + 1) * items_per_page] -%}\r\n\t\t<tr>\r\n\t\t\t<td style=\"width: 12%; text-align: center;\">\r\n\t\t\t    <div style=\"border: 0px;\"><b>{{ row.item_code or '' }}</b></div></td>\r\n\t\t\t<td style=\"width: 60%;\">\r\n\t\t\t\t<div style=\"border: 0px;\"><b>{{ row.description or '' }}</b></div></td>\r\n\t\t\t<td style=\"width: 6.5%; text-align: center;\"><b>{{ row.qty }}</b></td>\r\n\t\t\t<td style=\"width: 10%; text-align: center;\"><b>{{\r\n\t\t\t\trow.rate or ''}}</b></td>\r\n\t\t\t<td style=\"width: 8%; text-align: center;\"><b>{{\r\n\t\t\t\trow.amount or ''}}</b></td>\r\n\t\t\t<td style=\"width: 8%; text-align: center;\"><b>{{\r\n\t\t\t\trow.tax_amount or ''}}</b></td>\r\n\t\t\t<td style=\"width: 20%; text-align: center;\"><b>{{\r\n\t\t\t\t row.total_amount or ''}}</b></td>\r\n\t\t</tr>\r\n\t\t{%- endfor -%}\r\n\t</tbody>\r\n</table>\r\n\r\n{%- if not loop.last -%}\r\n<div class=\"page-break\"></div>\r\n{%- endif -%}\r\n\r\n<div class=\"ksa-vat-format col-xs-2 text-left fixed w3-container\" style=\"text-align: right; text-left: 0px;\" id=\"qr\">\r\n    <div class=\"qr-flex\">\r\n        <!--<img class=\"qr-code\" src={{doc.ksa_einv_qr}}>-->\r\n        <img class=\"qr-code\" src={{doc.custom_qr_code_attach}}>\r\n    </div>\r\n </div>\r\n <div class=\"col-xs-6 text-right fixed w3-container\" id=\"address\">\r\n    <b>{{ doc.address_display }}</b>\r\n </div>\r\n{%- if loop.last -%}\r\n    {%- if loop.index == total_pages -%}\r\n        <div id=\"totals\" class=\"fixed w3-container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-8 text-right\"><b>{{ doc.total or 0 }}</b></div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <!-- {%- for row in doc.sales_team -%}-->\r\n                <!-- <div class=\"col-xs-12 text-left\"><b>{{ row.sales_person or ''}}</b></div>-->\r\n                <!--{%- endfor -%}-->\r\n                <div class=\"col-xs-8 text-right\"><b>{{ doc.discount_amount or 0}}</b></div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-8 text-right\"><b>{{doc.total_taxes_and_charges or 0}}</b></div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <!--<div class=\"col-xs-6 text-right\"></div>-->\r\n                <div class=\"col-xs-8 text-right\"><b>{{ doc.grand_total or 0}}</b></div>\r\n            </div>\r\n        </div>\r\n    {%- endif -%}\r\n{%- endif -%}\r\n\r\n<div class=\"col-xs-2 text-left fixed w3-container\" style=\"text-align: right; text-left: 0px;\" id=\"sales\">\r\n    <div class=\"qr-flex\"> {%- for row in doc.sales_team -%}\r\n        <b>{{ row.sales_person or ''}}</b>{%- endfor -%}\r\n    </div>\r\n </div>\r\n <!-- \t{%- for row in doc.sales_team -%}-->\r\n <!--      <div class=\"col-xs-12 text-right\"><b>{{ row.sales_person or ''}}</b></div>-->\r\n\t<!--{%- endfor -%}-->\r\n\r\n{%- endfor -%}",
