 "css": "#qr.fixed {\r\n  position: fixed;\r\n  bottom: 19%;\r\n  left: 60px;\r\n  width: 210px; /* Ensure the size is adequate for scanning */\r\n  height: 210px;\r\n}\r\n\r\n.qr-code {\r\n  width: 200%; /* Ensure the QR code scales properly */\r\n  height: auto;\r\n}\r\n#totals  {\r\n  position: fixed;\r\n  bottom: 12.5%;\r\n  right: 0%;\r\n  /*margin-top: 40%;*/\r\n  /*width: 100%;*/\r\n  /*height: 100px;*/\r\n  line-height: 3.1;\r\n  font-size: 12;\r\n}\r\n#sales.fixed  {\r\n  position: fixed;\r\n  bottom: 12%;\r\n  left: 25;\r\n  width: 200px;\r\n  height: 100px;\r\n}\r\n#address.fixed{\r\n  position: fixed;\r\n  bottom: 27%;\r\n  right: 30%;\r\n  max-width: 200px;\r\n  max-height: 100px;\r\n}\r\n/*#grand-total  {*/\r\n/*position: absolute;*/\r\n/*  right: 100%;*/\r\n  /*margin-top: 40%;*/\r\n  /*width: 100%;*/\r\n  /*height: 100px;*/\r\n/*}*/\r\n.page-break{\r\n    page-break-after: always;\r\n}\r\n/*#page-number {*/\r\n    /*position: fixed;*/\r\n    /*bottom: 0;*/\r\n    /*left: 0;*/\r\n    /*right: 0;*/\r\n/*    margin: -2%;*/\r\n/*    display: block;*/\r\n/*    text-align: center;*/\r\n/*}*/",
