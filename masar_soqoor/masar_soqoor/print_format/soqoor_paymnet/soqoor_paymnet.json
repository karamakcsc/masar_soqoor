 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n    <!--{% if letter_footer and not no_letterfooter %}-->\r\n    <!--<div class=\"letter-head\">{{ letter_footer }}</div>-->\r\n    <!--{% endif %}-->\r\n    <!--{%- if doc.meta.is_submittable and doc.docstatus==0-%}-->\r\n    <!--<div class=\"alert alert-info text-center\">-->\r\n    <!--    <h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4></div>-->\r\n    <!--{%- endif -%}-->\r\n    <!--{%- if doc.meta.is_submittable and doc.docstatus==2-%}-->\r\n    <!--<div class=\"alert alert-danger text-center\">-->\r\n    <!--    <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>-->\r\n    <!--{%- endif -%}-->\r\n    <!--{% if max_pages > 1 %}-->\r\n    <!--<p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>-->\r\n    <!--{% endif %}-->\r\n{%- endmacro -%}\r\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\r\n<label>\r\n<p class=\"text-center\" style=\"font-size:20px; background-color:Gainsboro;\"><b>{{ _(\"Payment Voucher\") }}<br>{{doc.get_formatted(\"base_paid_amount\")}}</b></p><br>\r\n</label>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left-aligned\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Vou No.\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left-aligned\" style=\"font-weight: bold; text-align: left; text-left: 0px;\">{{doc.name or ''}}</div>\r\n            <!--<div class=\"col-xs-4 text-left\" style=\"text-align: left; text-left: 0px;\"><label>{{ _(\"Mode Of Payment\") }}:</label></div>-->\r\n            <!--<div class=\"col-xs-4 text-left\" style=\"font-weight: bold; text-align: left; text-left: 0px;\">{{ _(doc.mode_of_payment) or ''}}</div>          -->\r\n        </div>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-4 text-left-aligned\" style=\"text-align: left; text-left: 0px;\"><label>{{ _(\"Mode Of Payment\") }}:</label></div>\r\n            <div class=\"col-xs-6 text-left-aligned\" style=\"font-weight: bold; text-align: left;\">{{ _(doc.mode_of_payment) or ''}}</div>          \r\n        </div>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left-aligned\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Vou Date\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left-aligned\" style=\"font-weight: bold;text-align: left; text-left: 0px;\">{{doc.get_formatted(\"posting_date\") or ''}}</div>\r\n        </div>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left-aligned\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Paid To\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left-aligned\" style=\"font-weight: bold;text-align: left; text-left: 0px;\">{{doc.paid_from_des or ''}}</div>\r\n            \r\n        </div>\r\n<div class=\"row\">      \r\n           <div class=\"col-xs-2 text-left-aligned\" style=\"text-align: right; text-right: 0px;\"><label>{{ _(\"Bank IBan\") }}:</label></div>\r\n           <div class=\"col-xs-4 text-left-aligned\" style=\"font-weight: bold;text-align: right; text-right: 0px;\">{{ _(doc.reference_no) or ''}}</div>\r\n\r\n          </div>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left-aligned\" style=\"text-align: right; text-right: 0px;\"><label>{{ _(\"Cr Account\") }}:</label></div>\r\n            <div class=\"col-xs-6 text-left-aligned\" style=\"font-weight: bold;text-align: right; text-right: 0px;\">{{doc.paid_from or ''}}</div>\r\n\r\n            </div>\r\n<!--<div class=\"row\">-->\r\n<!--            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-right: 0px;\"><label>{{ _(\"Paid To\") }}:</label></div>-->\r\n<!--            <div class=\"col-xs-6 text-left\" style=\"font-weight: bold;text-align: right; text-left: 0px;\">{{doc.company or ''}}</div>-->\r\n\r\n<!--            </div>-->\r\n<table class=\"table table-bordered\">\r\n        <tr>\r\n            <th class=\"text-center\">{{ _(\"Account No.\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Paid To\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Desc\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Amount\") }}</th>\r\n         </tr>   \r\n        <tr>\r\n            <td style=\"width: 20%;font-size: 10px;; text-align: left;\"><b>{{doc.party}}</b></td>\r\n            <td style=\"width: 25%;font-size: 10px;; text-align: left;\"><b>{{doc.party_name}}</b></td>\r\n            <td style=\"width: 40%;font-size: 10px;; text-align: left;\"><b>{{doc.remarks}}</b></td>\r\n            <td style=\"width: 15%;font-size: 10px;; text-align: left;\"><b>{{doc.get_formatted(\"base_paid_amount\")}}</b></td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n\r\n<div class=\"row\">\r\n            <div class=\"col-xs-16 text-right\"></div>\r\n            <div class=\"col-xs-9 text-right\"><label>{{ _(\"Total\") }}</label></div>\r\n            <div class=\"col-xs-3 text-right\" style=\"font-weight: bold; text-align: left; text-left: 0px;\">{{doc.get_formatted(\"base_paid_amount\")}}</div>\r\n       </div> \r\n                <br>\r\n         <br>\r\n         <br>\r\n         <br>\r\n         <br>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-right: 0px;\"><label>{{ _(\"Recipient\") }}:</label></div>\r\n            <div class=\"col-xs-6 text-left\" style=\"width:40%;line-height: 3.0; text-align: left;\"><b>Name: ..................................</b><br><b>Signature: ............................</b></div>\r\n         </div>\r\n         <br>\r\n         <br>\r\n         <br>\r\n         <br>\r\n         <br>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-right: 0px;\"><label>{{ _(\"Prepared by\") }}:</label></div>\r\n            <div class=\"col-xs-6 text-left\" style=\"width:40%;line-height: 3.0; text-align: left;\"><b>Signature: ............................</b></div>\r\n         </div>\r\n         <br>\r\n         <br>\r\n         <br>\r\n         <br>\r\n         <br>\r\n         \r\n<div class=\"row\">\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: left; text-right: 0px;\"><label>{{ _(\"Financial Manager\") }}:</label></div>\r\n            <div class=\"col-xs-6 text-left-aligned\" style=\"width:40%;line-height: 3.0;text-align: left;\"><b>Signature: ............................</b></div>\r\n         </div>\r\n<br>\r\n\r\n\r\n<!--<div class=\"page-break\"></div>-->\r\n<!--{%- macro add_footer(page_num, max_pages, doc, letter_head, no_letterhead) -%}-->\r\n<!--{% if letter_head and not no_letterhead %}-->\r\n<!--       <div class=\"footer-html\" style=\"position: fixed; left: 0; bottom:0;width: 100%; text-align: center;\">{{ footer }}</div>-->\r\n<!--       {% endif %}-->\r\n       \r\n<!-- {%- endmacro -%}-->\r\n\r\n<!--{{ add_footer (1,1,doc,letter_head, no_letterhead) }}-->\r\n\r\n<!-- page footer -->\r\n<!--<table id=\"footer\" width=\"100%\">-->\r\n<!--  <tr>-->\r\n<!--    <td width=\"100%\">-->\r\n<!--      {{ footer }}-->\r\n<!--    </td>-->\r\n<!--  </tr>-->\r\n<!--</table>-->",
